{
    "github": {
        "silent": true
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm install",
    "rewrites": [
        { "source": "/share/posts/:id", "destination": "/api/share/posts/:id" },
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}